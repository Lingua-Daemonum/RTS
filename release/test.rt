
# get random hex color
getHex -> UInt
  result = "#" # hex color
  i = 0        # color cycle
  ? i < 6
    rand = randUInt(0,15)
    ? rand = 10
      rand = 'a'
    ? rand = 11
      rand = 'b'
    ? rand = 12
      rand = 'c'
    ? rand = 13
      rand = 'd'
    ? rand = 14
      rand = 'e'
    ? rand = 15
      rand = 'f'
    result += rand
    i += 1
    go(1)
  = result

a = getHex()
println(f"{a}:{type(a)}")
b = getHex()
println(f"{b}:{type(b)}")
c = getHex()
println(f"{c}:{type(c)}")
println(f"{a} {b} {c}")